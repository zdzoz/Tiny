function mod(x,y); {
    if y == 0 then
        return x;
    fi;
    while x < 0 do
        let x <- x + y;
    od;
    while x >= y do
        let x <- x - y;
    od;
    return x;
};

function gcd(x,y); {
    if x == 0 then
        return y;
    fi;
    return call gcd(y, call mod(x,y));
};

